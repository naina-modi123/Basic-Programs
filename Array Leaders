class Solution {
    static ArrayList<Integer> leaders(int arr[]) {
        // code here
        int n = arr.length;
        ArrayList<Integer> ans = new ArrayList<>();
        int max = Integer.MIN_VALUE;
        for(int i= n-1;i>=0;i--){
            if(arr[i] >= max){
                max = arr[i];
                ans.add(arr[i]);
                
            }
        }
        Collections.reverse(ans);
        return ans;
    }
}
